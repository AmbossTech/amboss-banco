{
  "ci": {
    "collect": {
      "startServerCommand": "node .next/standalone/server.js",
      "startServerReadyPattern": "ready on",
      "url": ["http://localhost:3000/"],
      "numberOfRuns": 1,
      "settings": {
        "preset": "desktop"
      }
    },
    "assert": {
      "preset": "lighthouse:recommended"
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "server": {}
  }
}
